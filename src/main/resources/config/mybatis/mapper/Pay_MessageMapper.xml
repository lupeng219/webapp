<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baibao.web.p2p.generator.dao.Pay_MessageMapper">



	<select id="findPayMessageByChannel" parameterType="java.lang.String"
		resultType="com.baibao.web.p2p.generator.bean.P2p_Pay_Message">
		SELECT * FROM p2p_pay_message as m
		WHERE m.pay_channel = #{channel}
	</select>


</mapper>